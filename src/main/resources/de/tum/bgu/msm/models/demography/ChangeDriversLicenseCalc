    } else if (personType.name() == "menAge90to94"){
         return 0.00;
    } else if (personType.name() == "menAge95to99"){
         return 0.00;
    } else if (personType.name() == "menAge100plus"){
         return 0.00;
    } else if (personType.name() == "womenAge15to19"){
