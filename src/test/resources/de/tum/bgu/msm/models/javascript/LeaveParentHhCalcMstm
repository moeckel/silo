var calculateLeaveParentsProbability = function(personType) {

    if (personType > 43){
        throw new Error("Undefined person type!")
    } else if (personType < 0){
        throw new Error("Undefined person type!")
    }

    if (personType == 4){
        return alpha = 0.0143;
    } else if (personType == 5){
        return alpha = 0.0398;
    } else if (personType == 6){
         return alpha = 0.0214;
    } else if (personType == 7){
         return alpha = 0.0064;
    } else if (personType == 8){
         return alpha = 0.0018;
    } else if (personType == 9){
         return alpha = 0.0006;
    } else if (personType == 10){
         return alpha = 0.0001;
    } else if (personType == 26){
         return alpha = 0.0202;
    } else if (personType == 27){
         return alpha = 0.0432;
    } else if (personType == 28){
         return alpha = 0.0192;
    } else if (personType == 29){
         return alpha = 0.0038;
    } else if (personType == 30){
         return alpha = 0.0006;
    } else if (personType == 31){
         return alpha = 0.0003;
    } else if (personType == 32){
         return alpha = 0.0001;
    }
    return 0.;
}